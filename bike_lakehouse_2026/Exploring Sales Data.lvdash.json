  "datasets": [
    {
      "name": "be610d37",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `workspace`;\n",
        "USE SCHEMA `sales_db`;\n",
        "\n",
        "\n",
        "--total orders over time\n",
        "SELECT date_trunc('month',order_date) AS orderdate,\n",
        "SUM(sales_amount)\n",
        "FROM workspace.sales_db.fact_sales\n",
        "GROUP BY date_trunc('month',order_date)"
      ]
    },
    {
      "name": "31a8124d",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `workspace`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "SELECT \n",
        "SUM(sales_amount) AS Sales,\n",
        "p.category AS category \n",
        "FROM workspace.sales_db.fact_sales2 f\n",
        "LEFT JOIN workspace.sales_db.dim_products p\n",
        "ON f.product_key = p.product_key\n",
        "GROUP BY p.category"
      ]
    }
  ],
